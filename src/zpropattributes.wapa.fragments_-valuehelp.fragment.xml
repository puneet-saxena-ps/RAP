<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.publicstorage.prop.zpropattributes.controller.Main" >                                                                                                                      
	<SelectDialog class="sapUiSizeCompact"                                                                                                                                                                                                                        
		id="valuehelp"                                                                                                                                                                                                                                               
		title="Value Help Selection"                                                                                                                                                                                                                                 
		items="{helpModel>/}"                                                                                                                                                                                                                                        
		search="onValueHelpDialogSearchCode"                                                                                                                                                                                                                         
		confirm="onValueHelpDialogClose"                                                                                                                                                                                                                             
		cancel="onValueHelpDialogClosePlant">                                                                                                                                                                                                                        
		<StandardListItem                                                                                                                                                                                                                                            
			class="sapUiSizeCompact"                                                                                                                                                                                                                                    
			iconDensityAware="false"                                                                                                                                                                                                                                    
			iconInset="false"                                                                                                                                                                                                                                           
			title="{helpModel>Keyfield}"/>                                                                                                                                                                                                                              
	</SelectDialog>                                                                                                                                                                                                                                               
</core:FragmentDefinition>                                                                                                                                                                                                                                     